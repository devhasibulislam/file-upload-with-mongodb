{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/upload/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/files",
      "dest": "index.js"
    },
    {
      "src": "/download/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/rename/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/delete/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "ATLAS_URI": "@atlas_uri"
  }
}
